/* ========================================   MENU   ============================================================== */

#apollo-menu{
    font-family: $megamenu-font-family;
    background: $megamenu-bg-outsite;
    color: $megamenu-color;
  	padding: 0px;
    margin: auto;
}
.apollo-megamenu{
	ul, ol {
        list-style: outside none none;
        margin: 0px;
        padding: 0px;
      	position: relative;
    }
}
#apollo-top-menu{    
    position: relative;
    display: block;
    text-align: center;
    background-color: $megamenu-bg;
  	padding: 0;
}
#apollo-menu{
    .navbar{
        background: $megamenu-bg;
        border: 0;
        margin: 0;
        padding: 0;
        @include rounded-corners(0);
    }
    /* menu level 1 */
    .navbar-nav{
        > li {
          display: inline-block;
          float: none;
          padding:0px;
          margin:0px;
          position: relative;
          border-left: 1px solid #1e1e1e;
            &.full-width {
                position: static;
                > .dropdown-menu  {
                    width: 100% !important;
                }
            }
            &.aligned-center {
                > .dropdown-menu  {
                    left: 50%;
                    @include translate(-50%, 0);
                    .rtl &{
                    	@include translate(50%, 0);
                    }
                }
            }
          &.aligned-center:nth-child(2){
            @include at-query (null,$screen-lg - 176, $screen-lg - 48){
              > .dropdown-menu  {
                left: 80%;
                @include translate(-80%, 0);
              }
            }
          }
            > a{
                position: relative;
              	padding: $megamenu-parent-padding;              
                font-weight: $megamenu-font-weight;
              	font-size: $megamenu-parent-font-size;
                text-transform:  $megamenu-parent-text-transform;
                background-color: transparent;
              	color: $megamenu-link-color;
                
              &:after{
                content: "";
                    background: #ffd200;
                height: 3px;
                width: 1px;
                position: absolute;
                bottom: 0px;
                left: 0px;
                right: 0px;
                margin: auto;
                -webkit-transition: all ease 0.5s;
                -o-transition: all ease 0.5s;
                transition: all ease 0.5s;
                opacity: 0;
                filter: alpha(opacity=0);
              }
                &:hover,&:focus,&:active{                   
                    color: $megamenu-link-hover-color;              
                    background-color: $megamenu-link-bg-hover-color;
                 }
              &:hover{
                &:after{
                  width: 100%;
                  opacity: 1;
                  filter: alpha(opacity=100);
                }
              }

            }
/*             &.dropdown:hover{
              	>a{
                  	&:after{
                    	border-bottom-width: 6px;
                      	@include opacity(1);
                      	@include transition(border 300ms ease-out 0s);
                    }
                }
            } */
            .caret{
              	&:before{
                  	font-size: 0px;
                	content: "\f107";
                  	font-family: $font-icon;
                  	position: absolute;
                  	top: 35px;
                  	right: 20px;
                }
            }
          &:first-child{
            border: none;
          }
        }
    }
    /* level 2 */
    .dropdown-menu{         
        top: 150%;  
        left: 0px;
        display: block;
        visibility: hidden;
        height: auto;
        font-weight: 300;
        @include opacity(0);
        color: $megamenu-sub-text-color;
        border: $megamenu-sub-border;
        min-width: $megamenu-sub-min-width;
        padding: $megamenu-sub-padding;
        background-color: $megamenu-sub-bg-color;                           
        @include rounded-corners($megamenu-sub-radius);                         
        @include box-shadow($megamenu-sub-shadow);
        @include transition(all 0.3s linear 0s);
      	@include rtl-text-align-left();
        li{
            line-height: 22px;
            padding: 3px 0;
            a{    
                display: block;
                color:$megamenu-sub-link-color;
                padding: 0 15px;
                font-weight: normal;
                font-size: $megamenu-sub-font-size - 1;
                line-height: 28px;              
                &:hover{
                    background-color: transparent;
                    color: $megamenu-sub-link-hover-color;
                    .fa{
                        color: $megamenu-sub-link-hover-color;
                    }
                }
              	.caret{
                    &:before{
                        content: "\f105";
                        top: 2px;
                      	right: 20px;
                    }
                }
            }           
        }       
        .menu-title{
            margin: 5px 0px 20px;
          	padding-bottom:10px;
          	border-bottom:2px solid #e8e8e8;
          	font-weight: bold;
            font-size: $theme-font-size;
            text-transform: uppercase;
            color: $megamenu-heading-color;
            letter-spacing: $megamenu-heading-letter;
          	a{
            	color: $megamenu-heading-color;
              	&:hover{color: $megamenu-sub-link-hover-color;}
            }
        }
        a > .menu-title{
            text-transform: none;
            color: $megamenu-sub-text-color;
        }
    }
    .dropdown-sub{
        padding: 20px;
        li{
            a{
                padding: 0px;
            }
        }
    }
    .dropdown{      
        &:hover{
            > .dropdown-menu{                       
                visibility: visible;
                top: 100%;
                @include opacity(1);
                @include transition(all 0.3s linear 0s);
            }
        }       
    }
    /* level > 3 */
    .dropdown-submenu{
        position: relative;
        .dropdown-menu{                     
            top: 0;
            right: auto;
             @include rtl-left(120%);                
            position: absolute;
            display: block;
            visibility: hidden;
            @include opacity(0);
            @include transition(all 0.4s ease);
        }
      	
        &:hover{
            > .dropdown-menu{    
                visibility: visible;        
                @include rtl-left(100%);  
                @include opacity(1);
                @include transition(all 0.4s ease);
            }
        }
    }
    .mega-group {       
        & > .dropdown-toggle {          
            border: 0;  
            display: block;         
            letter-spacing: 3px;                
            text-transform: uppercase;                  
            font-family: $megamenu-heading-title-font-family;   
            color: $megamenu-heading-color;         
            .fa{
                color: $white;
            }
        }   
    } 

    .megamenu .cols1{
        min-width:200px;
    }
    .megamenu .cols2{
        min-width:500px;
    }
    .megamenu .cols3{
        min-width:740px;
    }
/*     product  megamenu*/
  .products_menu{
    .content_price{
      position: static;
      @include rtl-text-align-left();
      .price-old, .old-price{
      	display: inline-block;
      }
      .price{
      	display: inline-block;
      }
    }
  }
    .product-block{
        background: $nocolor;
        margin: 0px;
