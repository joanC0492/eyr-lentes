function updateOptionsInSelector(t, sel) {
    var from = (sel != "quickview") ? "" : ".quick-view-product ";
    switch (t) {
        case 0:
            var n = "root";
            var r = $(from + ".single-option-selector:eq(0)");
            break;
        case 1:
            var n = $(from + ".single-option-selector:eq(0)").val();
            var r = $(from + ".single-option-selector:eq(1)");
            break;
