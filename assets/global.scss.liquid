            font-size:$theme-font-size - 2;
            margin: 0px;
        }
    }

    .btn-menu-canvas{
        background-color: $nocolor;
        color: $white;
        border: 1px solid $white;
        margin: 5px;
        @include transition(all 0.35s ease 0s);
      @media (max-width:767px){
       margin-top:-50px;
      }
        &:hover{
            background-color: white;
