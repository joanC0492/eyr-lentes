#search-icon{
  position: absolute;
  right: 10px;
  top: 25px;
  z-index: 1;
  display: inline-block;
  background-image: none;
  background: none;
  -moz-box-sizing: content-box;
  border: 0;
  box-shadow: none;
  line-height: inherit;
  margin: 0;
  padding: 0;
  text-decoration: none;
  text-shadow: none;
  font-weight: 400;
}
#search-icon-close{
  z-index: 99999;
