  cursor: pointer;
  font-size: 40px;
  color: #000;
  display: inline-block;
  position: fixed;
  top: 10px;
  @include transform(scale(0));
  @include transition(all 0.4s cubic-bezier(0.25,0.46,0.45,0.94));  
  right: 20px;
  &.show{
    @include transform(scale(1));
  }
}

.over-layer {
  @include transition(all 0.6s cubic-bezier(0.25,0.46,0.45,0.94));
  width: 60px;
  height: 60px;
  position: fixed;
  top: -60px;
  right: -60px;
  z-index: 9998;
	&:before{
    content: "";
    left: 0;
    top: 0;
    position: absolute;
    width: 100%;
    height: 100%;
    @include rgba($white,0.9);    
    @include rounded-corners(100%);  
    @include transition(all 0.6s cubic-bezier(0.25,0.46,0.45,0.94));        
      z-index: 9999;
   }
  &.show{
    &:before{
      @include transform(scale(75)); 
    }
  }     
}

