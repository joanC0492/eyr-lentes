{{ 'jquery-1.11.3.min.js' | asset_url | script_tag }}
{% assign pagewishlist = pages[settings.theme_wishlist_page] %}
<script>
  var theme = {
    strings: {
      addToCart: {{ 'products.product.add_to_cart' | t | json }},
      soldOut: {{ 'products.product.sold_out' | t | json }},
      unavailable: {{ 'products.product.unavailable' | t | json }},
      showMore: {{ 'general.filters.show_more' | t | json }},
      showLess: {{ 'general.filters.show_less' | t | json }}
    },
    moneyFormat: {{ shop.money_format | json }},
    quickview_enable: {{ settings.theme_quickview_enable }},
    wishlist_enable: {{ settings.theme_wishlist_enable }},
    wishlist_url: '{{ pagewishlist.url }}',
    shop_currency: '{{ shop.currency }}',
    file_url: '{{''|file_url}}',
    asset_url: "{{'Apollo'|asset_url|remove:"Apollo"}}",
    multiple_filter_sidebar_enable: {{ settings.use_multiple_filter }},
    money_currency_format: "{{shop.money_with_currency_format}}"
  }  
</script>
