}

/*============================================================================
  Slick slider overrides
==============================================================================*/
$slick-dot-size: 12px;
$slick-dot-size-small: 10px;

.slick-dotted.slick-slider {
  margin-bottom: 0;
}

/*================ Slick dots and prev/next pagination ================*/
.slick-slider .slick-dots {
  margin: 20px 0;
  width: auto;
  li {
    margin: 0;
    vertical-align: middle;
    width: $slick-dot-size-small;
    height: $slick-dot-size-small;
		@include rtl-margin-left(6px);
    &:first-of-type {
      margin-left: 0;
    }

    @include at-query($min,$screen-desktop) {
      width: $slick-dot-size;
      height: $slick-dot-size;
      @include rtl-margin-left(8px);
    }
    button {
      position: relative;
      padding: 0;
      width: $slick-dot-size-small;
      height: $slick-dot-size-small;
      @include at-query($min,$screen-desktop) {
        width: $slick-dot-size;
        height: $slick-dot-size;
      }
      &:before{
        text-indent: -9999px;
        background-color: transparent;
        border-radius: 100%;
        background-color: currentColor;
        width: $slick-dot-size-small;
        height: $slick-dot-size-small;
        transition: all 0.2s;
        background: #dddddd;
        @include at-query($min,$screen-desktop) {
          width: $slick-dot-size;
          height: $slick-dot-size;
        }
      }
    }
    &.slick-active button::before {
      opacity: 1;
      background: $color-slideshow-dots-active;
    }
    button:active::before {
      opacity: 0.7;
    }
  }
}

/*================ Index sections ================*/
.index-section {
  padding-top: $section-spacing-small;
  padding-bottom: $section-spacing-small;
  @include at-query($min,$screen-desktop) {
    padding-top: $section-spacing - 25;
