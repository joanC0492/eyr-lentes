    }
    // manufacture
    .manu-logo{
        img{
            border: $main-border;
            margin-bottom: 10px;
            margin-right: 10px;
        }
    }
    //blockhtml
    .block-html{
        .box-image{
            margin-bottom: 15px;
        }
