{% if settings.theme_loading_enable %}
<noscript id="deferred-styles">
	{% if settings.theme_font_family  == settings.theme_font_family_tertiary and  settings.theme_font_family == settings.theme_font_family_senary  and  settings.theme_font_family  == settings.theme_font_family_senary %}
	<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.theme_font_family | remove: 'Google+' }}:300,400,500,700,800,900">
	{% else %}
		{% if settings.theme_font_family contains 'Google' %}
			<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.theme_font_family | remove: 'Google+' }}:300,400,500,700,800,900">
		{% endif %}
		{% if settings.theme_font_family_tertiary contains 'Google' %}
			<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.theme_font_family_tertiary | remove: 'Google+' }}:300,400,500,700,800,900">
		{% endif %}
		{% if settings.theme_font_family_septenary contains 'Google' %}
			<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.theme_font_family_septenary | remove: 'Google+' }}:300,400,500,700,800,900">
		{% endif %}
		{% if settings.theme_font_family_senary contains 'Google' %}
			<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.theme_font_family_senary | remove: 'Google+' }}:300,400,500,700,800,900">
		{% endif %}
	{% endif %}
  {{ 'global.scss.css' | asset_url | stylesheet_tag  }}
  {% if settings.filter_price_bar_enable %}
		{{ 'jquery-ui.min.css' | asset_url | stylesheet_tag  }}
	{% endif %}
</noscript>
{% else %}
	{% if settings.theme_font_family  == settings.theme_font_family_tertiary and  settings.theme_font_family == settings.theme_font_family_senary  and  settings.theme_font_family  == settings.theme_font_family_senary %}
	<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.theme_font_family | remove: 'Google+' }}:300,400,500,700,800,900">
	{% else %}
		{% if settings.theme_font_family contains 'Google' %}
			<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.theme_font_family | remove: 'Google+' }}:300,400,500,700,800,900">
		{% endif %}
		{% if settings.theme_font_family_tertiary contains 'Google' %}
			<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.theme_font_family_tertiary | remove: 'Google+' }}:300,400,500,700,800,900">
		{% endif %}
		{% if settings.theme_font_family_septenary contains 'Google' %}
			<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.theme_font_family_septenary | remove: 'Google+' }}:300,400,500,700,800,900">
		{% endif %}
		{% if settings.theme_font_family_senary contains 'Google' %}
			<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.theme_font_family_senary | remove: 'Google+' }}:300,400,500,700,800,900">
		{% endif %}
	{% endif %}
	{{ 'global.scss.css' | asset_url | stylesheet_tag  }}
	{% if settings.filter_price_bar_enable %}
		{{ 'jquery-ui.min.css' | asset_url | stylesheet_tag  }}
