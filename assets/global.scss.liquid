// ----------------------------- widget Images  ----------------------- //
 .widget-images{
   a{
      &:before{
        position: absolute;
        content: "";
        top: 0;
        left: 0;
        width: 0;
        height: 0;
        margin: auto;
        background-color: rgba(255,255,255,0.1);
        -webkit-transition: all 0.3s ease-out 0s;
        transition: all 0.3s ease-out 0s;
      }
      &:after{
        content: "";
        position: absolute;
        right: 0;
        bottom: 0;
        width: 0;
        height: 0;
        background-color: rgba(255,255,255,0.1);
        -webkit-transition: all 0.3s ease-out 0s;
        transition: all 0.3s ease-out 0s;
      }
      &:hover{
        &:before,&:after{
          width: 100%;
          height: 100%;
        }
      }
    }
 }