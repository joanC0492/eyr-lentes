        &,&:hover{
            text-decoration: none;
        }
    }
    >li{
        position: relative;
        > a, > span{
            text-overflow: ellipsis;
            white-space: nowrap;
            overflow: hidden;
            color: inherit;
            line-height: 20px;
            display: block;
            padding: 10px 10px 10px 20px;
            margin: 0;
            &:hover{
                color: $theme-color-secondary;
            }
        }
        &:not(.mm-divider){
            &:after{
                content: '';
                border-bottom-width: 1px;
                border-bottom-style: solid;
                display: block;
                position: absolute;
                left: 20px;
                right: 0;
                bottom: 0;
            }
        }
    }
    .mm-next{
        background: rgba(3, 2, 1, 0);
        width: 50px;
        padding: 0;
        position: absolute;
        right: 0;
        top: 0;
        bottom: 0;
        z-index: 2;
        &:before{
            content: '';
            border-left-width: 1px;
            border-left-style: solid;
            display: block;
            position: absolute;
            top: 0;
            bottom: 0;
            left: 0;
