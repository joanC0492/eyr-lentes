            border: none;
        }
      .product-meta{
        width: 70%;
        padding: 0px 10px;
        @include rtl-float-left();
        @include rtl-text-align-left();
        border:0;
        .product-container {
          margin-bottom: 15px;
        }
        h5{  
          max-height: 42px;
