          .product-name{
            font-weight: 400;
            font-size: $theme-font-size - 2;
            color: $megamenu-sub-link-color;
            line-height: 22px;
            &:hover{
              color: $megamenu-sub-link-hover-color;
            }
          }
        }
        .product-desc,.tooltip{
